{
    "name": "cylmat/symplay",
    "description": "",
    "type": "project",
    "license": "free",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "3.*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "easycorp/easyadmin-bundle": "^4.4",
        "friendsofsymfony/rest-bundle": "^3.5",
        "friendsofsymfony/user-bundle": "^3.1",
        "liip/imagine-bundle": "^2.10",
        "nelmio/cors-bundle": "^2.2",
        "predis/predis": "^2.1",
        "snc/redis-bundle": "^4.6",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^2.2",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/messenger": "6.3.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/workflow": "6.3.*",
        "symfony/yaml": "6.3.*",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.12",
        "behat/mink": "^1.10",
        "behat/mink-goutte-driver": "^2.0",
        "brianium/paratest": "^6.7",
        "dg/bypass-finals": "^1.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/symfony-extension": "^2.3",
        "friends-of-phpspec/phpspec-code-coverage": "^6.1",
        "jetbrains/phpstorm-stubs": "^2022.2",
        "kint-php/kint": "^5.0",
        "nikic/php-parser": "^4.15",
        "pestphp/pest": "^1.22",
        "phpmd/phpmd": "^2.13",
        "phpro/grumphp": "^1.15",
        "phpspec/phpspec": "^7.3",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "phpunitgen/console": "^4.2",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/maker-bundle": "1.40",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/web-profiler-bundle": "6.3.*"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "pestphp/pest-plugin": true,
            "phpro/grumphp": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\AppBundle\\": "src/AppBundle/",
            "App\\AppData\\": "src/AppData/",
            "App\\Admin\\": "src/Admin/",
            "App\\Cloud\\": "src/Cloud/",
            "App\\Data\\": "src/Data/",
            "App\\Encrypt\\": "src/Encrypt/",
            "App\\Front\\": "src/Front/",
            "App\\Script\\": "src/Script/",
            "App\\Text\\": "src/Text/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/AppTestCase.php",
            "tests/RepositoryTestCase.php"
        ],
        "psr-4": {
            "App\\Features\\": "features/bootstrap/",
            "App\\Tests\\": "tests/",
            "App\\Tests\\AppBundle\\": "tests/AppBundle/",
            "App\\Tests\\AppData\\": "tests/AppData/",
            "App\\Tests\\Admin\\": "tests/Admin/",
            "App\\Tests\\Cloud\\": "tests/Cloud/",
            "App\\Tests\\Data\\": "tests/Data/",
            "App\\Tests\\Encrypt\\": "tests/Encrypt/",
            "App\\Tests\\Front\\": "tests/Front/",
            "App\\Tests\\Script\\": "tests/Script/",
            "App\\Tests\\Text\\": "tests/Text/",
            "spec\\App\\": "spec/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        },
        "runtime": {}
    }
}
