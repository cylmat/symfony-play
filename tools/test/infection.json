{
    "$schema": "https://raw.githubusercontent.com/infection/infection/0.26.6/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "{.*NotTested/.*}"
        ]
    },
    "phpUnit": {
        "configDir": "."
    },
    "logs": {
        "text": "var/logs/infection.log"
    },
    "mutators": {
        "@default": true
    },
    "minMsi": 80,
    "minCoveredMsi": 100
}